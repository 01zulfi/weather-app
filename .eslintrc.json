{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "quotes": ["error", "double"],
    "no-restricted-syntax": "off",
    "operator-linebreak": 0,
    "implicit-arrow-linebreak": "warn"
  }
}
